<MainScreen>:
    name: 'Main Screen'
    app: app
    
    MDBoxLayout:
        id: boxlayout
        orientation: 'vertical'
        md_bg_color: "#f4f9fc"

############################################################
########################## Top Bar #########################
############################################################


        BoxLayout:
            orientation: 'horizontal'
            padding: dp(15)
            spacing: dp(15)
            size_hint: 1, None
            size: self.minimum_size
            pos_hint: {"center_x": .5, "center_y": .5}

            MDLabel:
                text: "[color=11ac68]Green[/color] Clean"
                markup: True
                theme_text_color: "Custom"
                theme_font_name: "Custom"
                theme_font_size: "Custom"
                font_name:"BPoppins"
                text_color : "#0c1616"
                font_size : dp(25)
                halign: "left"


            MDIconButton:
                icon: "robot-excited"
                theme_icon_color: "Custom"
                theme_bg_color: "Custom"
                theme_font_size: "Custom"
                md_bg_color: "#f4f9fc"
                icon_color: "#282827" 
                font_size: dp(25)
                size_hint: None ,None
                size: "25dp", "25dp"
                on_release: app.go_to_second_screen() 
                pos_hint: {"center_x": .5, "center_y": .5}
                

            MDIconButton:
                icon: "information"
                theme_icon_color: "Custom"
                theme_bg_color: "Custom"
                theme_font_size: "Custom"
                md_bg_color: "#f4f9fc"
                icon_color: "#282827" 
                font_size: "25dp"
                size_hint: None ,None
                size: "25dp", "25dp"
                on_release: app.info_dialog()
                pos_hint: {"center_x": .5, "center_y": .5}

            MDIconButton:
                icon: "help-circle"
                theme_icon_color: "Custom"
                theme_bg_color: "Custom"
                theme_font_size: "Custom"
                md_bg_color: "#f4f9fc"
                icon_color: "#282827" 
                font_size: "25dp"
                size_hint: None ,None
                size: "25dp", "25dp"
                on_release: app.go_back_to_help_1_screen() 
                pos_hint: {"center_x": .5, "center_y": .5}




############################################################
############### Devices Menu & connect button ##############
############################################################
        BoxLayout:
            orientation: 'horizontal'
            padding: dp(7)
            spacing: dp(20)
            size_hint: None, None
            size: self.minimum_size
            pos_hint: {"center_x": .5, "center_y": .5}

            MDDropDownItem:
                id : menu
                on_release: app.open_devices_menu(self)
                bold:True
                font_name:"BPoppins"
                theme_icon_color: "Custom"
                theme_bg_color: "Custom"
                theme_font_size: "Custom"
                font_size: "20dp"
                size_hint: None ,None
                size: "40dp", "40dp"
                md_bg_color: "#11ac68"
                text_color: "#282827" 

                MDDropDownItemText:
                    id: drop_text
                    text: "Select Device"
                    font_name:"MPoppins"
                    bold:True
                    theme_icon_color: "Custom"
                    theme_bg_color: "Custom"
                    theme_font_size: "Custom"
                    size_hint: None ,None
                    size: "40dp", "40dp"
                    md_bg_color: "#11ac68"
                    text_color: "#282827" 
                    font_size : dp(20)


            MDIconButton:
                icon: "bluetooth-connect"
                text_color: "blue"
                on_release: app.connect_bluetooth()
                id :connect
                theme_icon_color: "Custom"
                theme_bg_color: "Custom"
                theme_font_size: "Custom"
                font_size: "30dp"
                size_hint: None ,None
                size: "45dp", "45dp"
                md_bg_color: "#fefefe"
                icon_color: "#196cac" 

        MDDivider:
